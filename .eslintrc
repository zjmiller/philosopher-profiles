{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
      "arrow-parens": "off",
      "camelcase": "off",
      "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
      "react/forbid-prop-types": "off",
      "react/jsx-pascal-case": "off",
      "react/sort-comp": "off",
      "jsx-a11y/no-static-element-interactions": "off"
    }
}
